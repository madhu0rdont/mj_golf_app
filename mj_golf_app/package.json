    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "katex": "^0.16.33",
    "lucide-react": "^0.574.0",
    "pg": "^8.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-katex": "^3.1.0",
    "react-router": "^7.13.0",
    "recharts": "^3.7.0",
    "swr": "^2.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
